
<div class="p-2 pt-0">
    <% for (const asdosExperience of asdosExperiences){ %>
        <div class="pb-5">
            <span class="text-xl highlighted">
                <%- asdosExperience.courseName %>
            </span>

            <div class="experience-detail-table cols-2-fit-content pt-1 pl-5">
                <span>
                    Term
                </span>
                <span>
                    <%- asdosExperience.courseTerm %>
                </span>


                <span>
                    Start
                </span>
                <span>
                    <%- asdosExperience.startingMonthYear %>
                </span>


                <span>
                    End
                </span>
                <span>
                    <%- asdosExperience.endingMonthYear %>
                </span>
            </div>
        </div>
    <% } %>
</div>
